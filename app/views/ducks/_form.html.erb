<%= form_with model: @duck do |f| %>
    <p> 
        <%= f.label :name %>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :description %>
        <%= f.text_field :description %> 
    </p>

    <p>
        <%= f.label :student %>
        <%= f.collection_select :student_id, Student.all, :duck_id, :name %>
    </p>

    <%= f.submit "Submit Duck", class: "button"%>

<% end %>