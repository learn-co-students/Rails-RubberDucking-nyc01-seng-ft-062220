<h1> Edit duck<h1>
<%=form_for @duck do |f| %>
    <%= f.label "Name:" %> 
    <%= f.text_field :name %>
    <%f.label "Description:" %>
    <%= f.text_field :description %> 
    <%= f.label :student_id %> 
    <%= f.collection_select :student_id, Student.all, :id, :name %>
    <%= f.submit %> 
    <%end%>
