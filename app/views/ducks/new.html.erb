<h1>New Duck Form</h1>

<% if flash[:my_errors] %>
    <% flash[:my_errors].each do |message| %>
        <%= message %><br>
<% end %>
<% end %>


<%= form_for @duck do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description%>
<%= f.text_area :description%>

<%= f.collection_select :student_id, @students, :id, :name %>

<%= f.submit%>
<%= %>
<%= %>
<% end %>
